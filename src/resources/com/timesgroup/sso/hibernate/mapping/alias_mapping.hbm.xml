<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.timesgroup.sso.hibernate.mapping">
	<class name="AliasMapping" table="ALIAS_MAPPING">
		<id name="aliasId" column="ALIAS_ID"></id>
		<property name="userId" column="USR_ID_VC" />
		<property name="userProfile" column="USR_PROFILE" />
		<property name="isPrimary" column="IS_PRIMARY" />
		<property name="userType" column="USR_TYPE" />
		<property name="hashCode" column="HASHCODE" />
		<property name="createdDate" column="CREATED_DATE" type="timestamp"/>
		<property name="updatedDate" column="UPDATED_DATE" type="timestamp"/>
	</class>
</hibernate-mapping>